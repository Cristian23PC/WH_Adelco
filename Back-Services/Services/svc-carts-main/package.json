{
  "name": "svc-carts",
  "version": "1.28.0",
  "description": "SVC Carts",
  "license": "MIT",
  "main": "./dist/src/main.js",
  "repository": {
    "type": "git",
    "url": "https://gitlab.com/adelco_corp/commerce/back-services/services/svc-carts.git"
  },
  "scripts": {
    "prenpm:login": "gcloud auth application-default login --project adelco-corp-cicd && gcloud config set project adelco-corp-cicd",
    "npm:login": "npx google-artifactregistry-auth",
    "start:local": "nest start --watch --trace-warnings",
    "start:dev": "node dist/src/main.js --trace-warnings",
    "prestart:prod": "rm -rf dist && tsc",
    "start:prod": "node dist/src/main.js --trace-warnings",
    "start": "node ./dist/src/main.js --trace-warnings",
    "start:debug": "nest start --debug --watch",
    "test": "jest --config ./jest.config.ts",
    "test:watch": "jest --watch",
    "test:cov": "jest --coverage",
    "test:e2e": "jest --config ./jest-e2e.config.ts",
    "format": "prettier --write \"src/**/*.ts\"",
    "lint": "eslint \"{src,apps,libs,test}/**/*.ts\" --fix",
    "tsc": "./node_modules/typescript/bin/tsc -p .",
    "prebuild": "rimraf dist",
    "build": "nest build --tsc",
    "gcp-build": "npm run build",
    "prepare": "is-ci || husky install",
    "semantic-release": "semantic-release",
    "commit": "git-cz",
    "prettier": "prettier --config .prettierrc --check src",
    "prettier:fix": "prettier --config .prettierrc --write src"
  },
  "dependencies": {
    "@adelco/lib_delivery": "^1.3.0",
    "@adelco/price-calc": "^1.7.0",
    "@commercetools/platform-sdk": "^4.6.0",
    "@commercetools/sdk-client-v2": "^2.1.4",
    "@google-cloud/pubsub": "^4.0.6",
    "@google-cloud/secret-manager": "^4.2.2",
    "@nestjs/axios": "^2.0.0",
    "@nestjs/cache-manager": "^2.1.0",
    "@nestjs/common": "^9.2.1",
    "@nestjs/config": "^2.3.1",
    "@nestjs/core": "^9.2.1",
    "@nestjs/platform-express": "^9.2.1",
    "@nestjs/swagger": "^6.1.4",
    "@nestjs/terminus": "^9.2.2",
    "@nestjs/typeorm": "^9.0.1",
    "axios": "^1.3.5",
    "class-transformer": "^0.5.1",
    "class-validator": "^0.14.0",
    "commercetools-sdk-repositories": "^1.0.3",
    "date-fns": "^2.30.0",
    "dotenv": "^16.0.0",
    "module-alias": "^2.2.2",
    "nestjs-pino": "^3.1.2",
    "pino-http": "^8.3.3",
    "pino-pretty": "^10.0.0",
    "typescript": "^4.6.2"
  },
  "devDependencies": {
    "@digitalroute/cz-conventional-changelog-for-jira": "^8.0.1",
    "@commitlint/cli": "^17.6.6",
    "@commitlint/config-conventional": "^17.6.6",
    "@nestjs/cli": "^9.3.0",
    "@nestjs/testing": "^9.2.1",
    "@semantic-release/changelog": "^6.0.3",
    "@semantic-release/commit-analyzer": "^10.0.1",
    "@semantic-release/exec": "^6.0.3",
    "@semantic-release/git": "^10.0.1",
    "@semantic-release/gitlab": "^12.0.3",
    "@semantic-release/release-notes-generator": "^11.0.3",
    "@types/jest": "^27.4.1",
    "@types/module-alias": "^2.0.1",
    "@types/node": "^17.0.21",
    "@types/supertest": "^2.0.12",
    "@typescript-eslint/eslint-plugin": "^5.15.0",
    "@typescript-eslint/parser": "^5.15.0",
    "commitizen": "^4.3.0",
    "commitlint": "^17.6.6",
    "cz-conventional-changelog": "3.3.0",
    "eslint": "^8.11.0",
    "eslint-plugin-import": "^2.26.0",
    "eslint-plugin-jsdoc": "^39.6.4",
    "eslint-plugin-prefer-arrow": "^1.2.3",
    "husky": "^8.0.2",
    "jest": "^27.5.1",
    "node-mocks-http": "^1.12.2",
    "prettier": "^2.6.0",
    "supertest": "^6.3.3",
    "timezone-mock": "^1.3.6",
    "ts-jest": "^27.1.3",
    "ts-node": "^10.7.0",
    "tsconfig-paths": "^3.14.0"
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/@digitalroute/cz-conventional-changelog-for-jira",
      "jiraLocation": "post-description",
      "skipScope": false,
      "jiraPrefix": "FP"
    }
  },
  "husky": {
    "hooks": {
      "commit-msg": "commitlint -E HUSKY_GIT_PARAMS",
      "pre-commit": "lint-staged"
    }
  },
  "engines": {
    "node": "18.14.0"
  }
}
